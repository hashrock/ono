/* layer: default */
.block{display:block;}
.max-w-md{max-width:28rem;}
.w-full{width:100%;}
.space-y-4>:not([hidden])~:not([hidden]){--un-space-y-reverse:0;margin-top:calc(1rem * calc(1 - var(--un-space-y-reverse)));margin-bottom:calc(1rem * var(--un-space-y-reverse));}
